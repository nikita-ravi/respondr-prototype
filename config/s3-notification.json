{
  "LambdaFunctionConfigurations": [
    {
      "Id": "RespondrDocProcessorTrigger",
      "LambdaFunctionArn": "LAMBDA_ARN_PLACEHOLDER",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "suffix",
              "Value": ".pdf"
            }
          ]
        }
      }
    }
  ]
}
